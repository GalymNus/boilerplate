{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "components": "./app/components/",
          "config": "./app/config/",
          "containers": "./app/containers/",
          "ducks": "./app/ducks/",
          "hocs": "./app/hocs/",
          "interfaces": "./app/interfaces/",
          "static": "./app/static/",
          "themes": "./app/themes/",
          "utils": "./app/utils/"
        }
      }
    ]
  ]
}
